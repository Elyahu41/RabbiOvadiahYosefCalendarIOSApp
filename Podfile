# Uncomment the next line to define a global platform for your project

target 'Rabbi Ovadiah Yosef Calendar' do
  use_frameworks!
  platform :ios, '14.0'

  pod 'KosherCocoa', '~> 3.6.0'
  pod 'SwiftyJSON', '~> 4.0'
end

target 'Rabbi Ovadiah Yosef Calendar WidgetExtension' do
  use_frameworks!
  platform :ios, '14.0'

  pod 'KosherCocoa', '~> 3.6.0'
end

target 'Rabbi Ovadiah Yosef Calendar watchOS Watch App' do
  use_frameworks!
  platform :watchos, '10.0'

  pod 'KosherCocoa', '~> 3.6.0'
end